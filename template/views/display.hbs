<a href="/insert">Insert Data</a>


<center>



<table>

    <tr>
        <td>
            ID
        </td>
        <td>
            ROLL NO
        </td>
        <td>
            NAME
        </td>
    </tr>

    {{#each data}}

       <form method="post" action="/update">
           <tr>
               <td>
                   <input readonly type="text" name="id" value="{{this._id}}">
               </td>
               <td>
                   <input type="text" name="roll" value="{{this.roll_no}}">
               </td>
               <td>
                   <input type="text" name="name" value="{{this.name}}">
               </td>
               <td>
                   <input name="btn" type="submit" value="UPDATE">
               </td>
               <td>
                   <input name="btn" type="submit" value="DELETE">
               </td>
           </tr>
       </form>

    {{/each}}


</table>

</center>